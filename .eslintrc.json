{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "warn",
    "max-len": ["warn", { "code": 140 }],
    "prefer-const": "warn",
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object"
        ]
      }
    ]
  }
}
